<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="CustomerPriceProductList">
        <property name="items" type="CustomerPriceProduct[]"/>
    </transfer>

    <transfer name="CustomerPriceProduct">
        <property name="idCustomerPriceProduct" type="int" />
        <property name="customerNumber" type="string" />
        <property name="productNumber" type="string" />
        <property name="quantity" type="int" />
        <property name="price" type="float" />
    </transfer>
</transfers>

